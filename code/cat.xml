<nta>
	<declaration>// Place global declarations here



const int max_data = 4;
typedef int[0,max_data] data_t;

const int data[data_t] = {3,2,7,1,4}; 


double a = 0.0;
double i = 0.0;

clock x;
void action()
{
    a = a + 5;
}</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-306" y="-561">
			<label kind="exponentialrate" x="-323" y="-552">1</label>
		</location>
		<location id="id1" x="-306" y="-408">
			<label kind="exponentialrate" x="-323" y="-399">1</label>
		</location>
		<location id="id2" x="-306" y="-315">
			<label kind="exponentialrate" x="-323" y="-306">1</label>
		</location>
		<location id="id3" x="-306" y="-213">
			<label kind="exponentialrate" x="-323" y="-204">1</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="guard" x="-297" y="-281">i &gt; 4.0</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="-136" y="-374">i &lt;= 4.0</label>
